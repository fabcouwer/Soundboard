{
  "name": "Soundboard",
  "version": "1.0.0",
  "description": "A soundboard to use with friends over the network.",
  "main": "src/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "budo index.js:public/js/bundle.js --serve js/bundle.js --live --open --dir ./public -v",
    "dev-server": "node server/index.js",
    "build": "browserify index.js -o public/js/bundle.js"
  },
  "dependencies": {
    "express": "^4.14.0",
    "music-metadata": "^0.7.10",
    "node-uuid": "^1.4.7",
    "play-sound": "^1.1.1",
    "ws": "^1.1.1"
  },
  "devDependencies": {
    "budo": "^9.2.1",
    "browserify": "^13.1.1"
  },
  "bundledDependencies": ["express", "music-metadata", "node-uuid", "play-sound", "ws"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jvanhees/Soundboard.git"
  },
  "engines": {
    "node": "8.8.1"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jvanhees/Soundboard/issues"
  },
  "homepage": "https://github.com/jvanhees/Soundboard#readme"
}
